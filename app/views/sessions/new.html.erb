<%= render 'partials/header' %>

<main>
    <div class="wrapper">
		<h1>Connexion</h1>

		<%= form_tag new_session_path do %>
			<div class="field">
			  <%= label_tag :email %><br>
			  <%= email_field_tag :email %>
			</div>
			<div class="field">
			  <%= label_tag :password %><br>
			  <%= password_field_tag :password %>
			</div>

			<%= submit_tag "Submit" %>
		<% end %>
	</div>
</main>
